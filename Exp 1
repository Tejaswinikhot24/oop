}

void Student::getdata()
{
  cout<<"Enter name of the Student :";
 cin>>name;

cout<<"Enter age of the student:";
cin>>age;

cout<<"Enter percentage :";
cin>>percentage;

}

int main()
{
  Student s1;
s1.getdata();


}

