#include <iostream>
using namespace std;

class Complex {

    public:
        int real, imag;
     public:
     Complex()
     {
          real=0;
          imag=0;
     }
    public:
    Complex (int r, int i)
     {
        real = r;
        imag = i;
     }

    Complex operator +(Complex c )
     {
         
           int r1 = real+c.real;
           int r2 = imag+c.imag;
     }
     void display()
     {
          cout<<real<<" "<<imag<<" ";
     }
     
};

int main()
{
  Complex c2(8,5),c(4,1),c1;
  c1=c2+c;
  c1.display();+
 
return 0;
}
